[event-agent]
namespace = <%= @ns %>
user = <%= scope['openiosds::user'] %>
<% if @workers -%>
workers = <%= @workers %>
<% end -%>
<% if @concurrency -%>
concurrency = <%= @concurrency %>
<% end -%>
queue_url = <%= @_queue_url %>
handlers_conf = <%= scope['openiosds::sysconfdir'] %>/<%= @ns %>/<%= @type %>-<%= @num %>/oio-event-handlers.conf
tube = <%= @tube %>
acct_update = <%= @acct_update %>
acct_refresh_interval = <%= @acct_refresh_interval %>
rdir_update = <%= @rdir_update %>
log_facility = <%= @log_facility %>
log_level = <%= @log_level %>
log_name = <%= @_log_name %>
log_address = <%= @log_address %>
syslog_prefix = OIO,<%= @ns %>,<%= @type %>,<%= @num %>
